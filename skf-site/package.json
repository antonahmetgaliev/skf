{
  "name": "skf-site",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "dev": "ng serve --proxy-config proxy.conf.json",
    "build": "ng build",
    "start": "node server.js",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
